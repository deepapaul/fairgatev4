{% set documentsCount = (documentsCount is defined) ? documentsCount : 0 %} 
<div class="row">
    <div class="row-same-height">    
        <div class="col-md-4 col-sm-12 col-xs-12 col-md-height fg-nav-tab-border">
            <div class="btn-group">          
                <h3 class="page-title fg-page-title fg-left-zero">
                    {% if clubTab is defined %}                
                        <button id="fgContactdrop" data-toggle="dropdown" class="btn tableaction"> <i class="fa fa-bars fa-2x"></i></button>
                        <ul class="dropdown-menu pull-left " id="fgdropmenu" ></ul>
                    {% endif %}              
                    {{ clubName }}</h3>
            </div>
        </div>
        <div class="col-md-8 col-sm-12 col-xs-12 col-md-height col-bottom fg-nav-tab-border">    
            <ul id="paneltab" class="nav fg-nav-links data-more-tab fg-visibility-hidden">
                <li name="fg-dev-overview-tab" data-target="1" class="{{active=='cluboverview' ? 'active':''}}"> <a  href="{{path('club_overview',{'offset':offset,'clubId': clubId})}}" >{{'PANEL_TABS_OVERVIEW'|trans}}</a> </li>
                <li name="fg-dev-data-tab" data-target="2" class="{{active=='data' ? 'active':''}}"> <a  href="{{ path('club_data',{'offset':offset ,'clubid': clubId})}}" >{{'PANEL_TABS_DATA'|trans}}</a> </li>
                <li name="fg-dev-assignments-tab" data-target="3" class="{{active=='assignments' ? 'active':''}}"> <a  href="{{path('club_assignments',{'offset':offset,'clubid': clubId})}}" >{{'PANEL_TABS_ASSIGNMENTS'|trans}} <span class="badge badge-round badge-important count-document-tab"> {{asgmntsCount}} </span></a> </li>
                <li name="fg-dev-notes-tab" data-target="4" class="{{active=='clubnote' ? 'active':''}}"> <a  href="{{path('club_note',{'offset':offset,'clubid': clubId})}}" >{{'NOTES'|trans}} <span class="badge badge-round badge-important count-document-tab"> {{notesCount}} </span></a> </li>
                {% if (fgtwig.inArray('document',fgclub.get('bookedModulesDet'))) %}
                    <li  name="fg-dev-documents-tab" data-target="5" class="{{active=='clubdocuments' ? 'active':''}}"> <a  href="{{path('club_documents',{'offset':offset,'clubId': clubId})}}" >{{'PANEL_TABS_DOCUMENTS'|trans}} <span class="badge badge-round badge-important count-document-tab"> {{documentsCount}} </span></a> </li>
                    <li  name="fg-dev-log-tab" data-target="6" class="{{active=='loglisting' ? 'active':''}}"> <a  href="{{path('club_log',{'offset':offset,'clubId': clubId})}}" >{{ 'LOG'|trans }}</a> </li>
                {% else %}
                    <li  name="fg-dev-log-tab" data-target="5" class="{{active=='loglisting' ? 'active':''}}"> <a  href="{{path('club_log',{'offset':offset,'clubId': clubId})}}" >{{ 'LOG'|trans }}</a> </li>
                {% endif %}
                <li name="fg-dev-more-tab" class="datahideshow"> <a class="dropdown-toggle" data-toggle="dropdown" href="#">{%trans%}MORE{%endtrans%} <i class="fa fa-angle-down"></i></a>
                    <ul class="dropdown-menu fg-dropdown-more" role="menu">
                        <li name="fg-dev-overview-tab" data-target="1" class="{{active=='cluboverview' ? 'active':''}}"> <a href="{{path('club_overview',{'offset':offset,'clubId': clubId})}}" tabindex="-1">{{'PANEL_TABS_OVERVIEW'|trans}}</a> </li>
                        <li name="fg-dev-data-tab" data-target="2" class="{{active=='data' ? 'active':''}}"> <a href="{{ path('club_data',{'offset':offset ,'clubid': clubId})}}" tabindex="-1" >{{'PANEL_TABS_DATA'|trans}}</a> </li>
                        <li name="fg-dev-assignments-tab" data-target="3" class="{{active=='assignments' ? 'active':''}}"> <a href="{{path('club_assignments',{'offset':offset,'clubid': clubId})}}" tabindex="-1" >{{'PANEL_TABS_ASSIGNMENTS'|trans}} <span class="badge badge-round badge-important count-document-tab"> {{asgmntsCount}} </span></a> </li>
                        <li name="fg-dev-notes-tab"data-target="4" class="{{active=='clubnote' ? 'active':''}}"> <a href="{{path('club_note',{'offset':offset,'clubid': clubId})}}" tabindex="-1">{{'NOTES'|trans}} <span class="badge badge-round badge-important count-document-tab"> {{notesCount}} </span></a> </li>
                        {% if (fgtwig.inArray('document',fgclub.get('bookedModulesDet'))) %}
                            <li name="fg-dev-documents-tab" data-target="5" class="{{active=='clubdocuments' ? 'active':''}}"> <a href="{{path('club_documents',{'offset':offset,'clubId': clubId})}}" tabindex="-1">{{'PANEL_TABS_DOCUMENTS'|trans}} <span class="badge badge-round badge-important count-document-tab"> {% if documentsCount is defined%} {{documentsCount}}{% endif %} </span></a> </li>
                            <li name="fg-dev-log-tab" data-target="6" class="{{active=='loglisting' ? 'active':''}}"> <a  href="{{path('club_log',{'offset':offset,'clubId': clubId})}}" tabindex="-1" >{{ 'LOG'|trans }}</a> </li>
                        {% else %}
                            <li name="fg-dev-log-tab" data-target="5" class="{{active=='loglisting' ? 'active':''}}"> <a  href="{{path('club_log',{'offset':offset,'clubId': clubId})}}" tabindex="-1" >{{ 'LOG'|trans }}</a> </li>
                        {% endif %}
                    </ul>
                </li>
            </ul>
        </div>
    </div>
</div>
<script type="text/javascript">
    var singleClubId = {{clubId}} ;
    var url = '';
  
</script>
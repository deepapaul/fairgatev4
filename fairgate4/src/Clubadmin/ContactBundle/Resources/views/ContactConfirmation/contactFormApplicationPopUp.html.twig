<div class="modal-header fg-modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
    <h4 class="modal-title">{{fgclub.formatDate(createdAt,'datetime')}}</h4>
</div>
<div class="modal-body fg-modal-text">
    <div class="row">
        <div class="col-md-12">
            <div class="portlet-body form">
                <div class="form-body">
                    <div class="form-group fg-overview-list">
                        <dl>   
                            {% for el in data %} 
                                <dt>{{ el.fieldName }}</dt>
                                <dd>{% if(el.fieldType == 'fileupload' or el.fieldType == 'imageupload') %}
{#                                    {% var downloadFilePath =  downloadPath.replace("|placeholder|", el.fieldValue) %}#}
                                    {% if (status == 'DISMISSED') %}
                                        {{ el.fieldValue|raw }}
                                    {% else %}
                                        <a href="{{ el.fileUrl }}">{{ el.fieldValue|raw }}</a>
                                    {% endif %}
                                    {% elseif(el.fieldType == 'url')  %}
                                        <a target="_blank" href="{{ el.fieldValue }}">{{ el.fieldValue |raw  }}</a>
                                    {% elseif(el.fieldType == 'number') %}
                                        {{fgclub.formatNumber(el.fieldValueForPopup)}}
                                    {% else %}
                                        {{ el.fieldValueForPopup |raw }}
                                    {% endif %}
                                </dd>
                            {% endfor %}
                        </dl>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

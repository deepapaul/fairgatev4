                                                                    {% if newsLetterContent.imageLink|length > 0 %}
                                                                        <a href="{{ newsLetterContent.imageLink }}" target="_blank" ><img width="575" src="{{ newsLetterContent.path~newsLetterContent.image }}" style="max-width:97%; width: 575px;" /></a>
                                                                        {% else %}
                                                                        <img width="575" src="{{ newsLetterContent.path~newsLetterContent.image }}" style="max-width:97%; width: 575px;" >
                                                                    {% endif %}

                                                                </td>
                                                            </tr>
                                                        </table><br /><!-- Break Tag for row -->



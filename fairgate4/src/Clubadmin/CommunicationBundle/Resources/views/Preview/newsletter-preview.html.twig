                                                    {% if (sponsorContent.sponsorAds|length > 0 ) %}
                                                        {% include 'ClubadminCommunicationBundle:Preview:sponsor-content.html.twig' with {'newsLetterContent' : sponsorContent } %}
                                                    {% endif %}
                                                {% endfor %}
                                            {% endif %}
                                            <!-- Fixed Sponsor above contents:end ---> 

                                            <!-- Table of contents:begin --->
                                            {% if tableOfContents|length > 0 %}

<!-- BEGIN PAGE CONTENT-->
<div class="row">
  <div class="col-md-12">
    <div class="row">
        <div class="col-md-12  col-sm-12 col-xs-12">
            <div class="fg-createmail-fileupload hide fg-upload">
                <div class="drop">
                    {% trans %}DM_FILE_UPLOAD_TEXT{% endtrans %}
                    <input type="file" title="{% trans %}DM_FILE_UPLOAD_TEXT{% endtrans %}" class="ignore" name="upl" multiple id="upload-1" data-row-callback="fileUploader.afterUpload" data-uploadtemplate="imageUploadArticle" data-name="" method="post" data-action="{{path('ads_file_upload')}}"/>
                </div>
            </div>
        </div>
    </div>
  </div>
</div>
<script type="text/template" id="fileUploadError">
    <div class="col-md-2 col-sm-2 col-xs-12 fg-left-zero">
    </div>
    <div class="col-md-5 col-sm-5 col-xs-12 fg-left-zero">
        <p><%- data.name %> <span class="help-block"><%- error %></span></p>
    </div>
    <div class="col-md-2 pull-right deletediv removeUpload">
        <div class="closeico" data-deletable="checknew">
            <input type="checkbox" data-parentid="<%- id %>" class="make-switch ignore">
            <label for="isDeleted"></label>
        </div>
    </div>
</script>
<!-- END PAGE CONTENT-->

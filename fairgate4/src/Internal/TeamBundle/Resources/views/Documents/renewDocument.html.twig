        </div>
    </div>
</div>
                         
<script type="text/template" id="editDocUpload">
<div id="<%- id %>"> 
    <span class="fg-left-zero FL fg-file-name">
         <span><%- name %></span>
         <input class="dFExclude" type="text" style="display:none" name="uniquename" value="<%- value %>"/>
         <input class="dFExclude" type="text" style="display:none" name="filename" value="<%- name %>"/>
         <input class="dFExclude" type="text" style="display:none" name="size" value="<%- size_raw %>"/>
         &nbsp; <a href="javascript:void(0)" class="close removeUploadedFile"></a>
         <div class="clearfix"></div>
     </span>
    <div class="col-md-6 col-sm-6 col-xs-12 fg-left-zero fg-pad-top-10">
        <div class="fg-upload-progress" style="display: block;">
            <div class="progress progress-striped active">
                <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">
                    <span class="sr-only"></span>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
</div>
<div class="clearfix"></div>
</script>  

<script type="text/template" id="editdocument-uploader-errorlist-template">   
    <% if(errors.length > 0) { %>
        <% _.each(errors, function(error) { %>
            <% if (error == 0) { %>
                <div class="alert alert-danger alert-dismissible">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    {{'DOCUMENT_UPLOAD_FILETYPE_ERROR'|trans|replace({'{#filetype#}': 'image, doc, pdf, excel, power, archive, audio, video, web, text'})}}
                </div>
            <% } %>
        <% }) %> 
    <% } %>   
</script>             
<!-- END PAGE CONTENT-->

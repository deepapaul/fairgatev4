<script type="text/template" id="fg-dev-add-new-articleadmin"> 
    <div class="row fg-border-line sortables fg-dev-new-contact-div" >
                        
        <div class="col-md-5 col-sm-8 col-xs-8">
            <div class="input-icon"> <i class="fa fa-search"></i>
                <input id="include-role-admin-<%- randomNumber %>" type="text"  class="form-control input-sm dFExclude" placeholder = "{{'CHOOSE_CONTACT'|trans}}"/>
            </div>
        </div>
        <input class="fg-dev-auto-complete-val hide" name ="new-include-role-admin-<%- randomNumber %>" type="text" id="new-include-role-admin-<%- randomNumber %>" value=""/>
         
        <div class="fg-last-icon new_contact_team_right_rmv">
            <div class="closeico">
                <input type="checkbox" class="make-switch" id="select">
                <label for="select"></label>
            </div>
        </div>
    </div> 
</script>
<script type="text/template" id="fg-dev-add-new-team_admin"> 
    <div class="row fg-border-line sortables fg-dev-new-contact-div" >
        <div class="col-md-5 col-sm-8 col-xs-8 ">
            <div class="input-icon"> <i class="fa fa-search"></i>
        <% if( roleType == 'teamAdmin') { %>
                <input id="include-team-admin-<%- randomNumber %>"  name="include-team-admin-<%- randomNumber %>" type="text"  class="form-control input-sm dFExclude" />
        <% }else { %>
                <input id="include-wg-admin-<%- randomNumber %>"  name="include-wg-admin-<%- randomNumber %>" type="text"  class="form-control input-sm dFExclude" />
        <% } %>
            </div>
        </div>
        <% if( roleType == 'teamAdmin') { %>
            <input class="fg-dev-auto-complete-val hide" type="text"  name="new-include-team-admin-<%- randomNumber %>" id="new-include-team-admin-<%- randomNumber %>" value=""/>
            <% } else{ %>
            <input class="fg-dev-auto-complete-val hide" type="text"  name="new-include-wg-admin-<%- randomNumber %>" id="new-include-wg-admin-<%- randomNumber %>" value=""/>
            <% } %>
        <div class="col-md-6 col-sm-6 col-xs-12">
        {#data-selected-text-format="count>3"#}
            <select id="teams_<%- roleType%>_<%- randomNumber %>" title="{{'SELECT_DEFAULT'|trans}}" multiple name="teams_<%- roleType%>_<%- randomNumber %>" data-key="teams.<%- roleType%>.<%- randomNumber %>.teams" class="selectpicker form-control show-tick  ">
           
                
                <% _.each(teamList,function(team){ %> 
                    <% var select= "multiple" ; %>
                    <option value="<%- team.roleId %>" class=<%- select%> ><%- team.rTitle %></option>
                <% }); %>
            </select>   
        </div>
        <div class="fg-last-icon new_contact_team_right_rmv">
            <div class="closeico">
                <input type="checkbox" class="make-switch" id="select">
                <label for="select"></label>
            </div>
        </div>
    </div> 
</script>

<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="1">
                <source>CONFIRM_MESSAGE</source>
                <target>Thank you for registering with Fairgate. We have successfully created a new club account for you to try out in a second after you clicked on this confirmation link below. This link will be active for 7 days, after this the account will be removed and you will have to register again:</target>
            </trans-unit>
            <trans-unit id="2">
                <source>CONFIRM_BTN</source>
                <target>Confirm your eMail</target>
            </trans-unit> 
            <trans-unit id="3">
                <source>CONFIRM_MESSAGE2</source>
                <target>Or copy this link into your web-browser if the button above doesn't work:</target>
            </trans-unit> 
            <trans-unit id="4">
                <source>CONFIRM_MESSAGE3</source>
                <target>Bei Fragen stehen wir gerne unter <![CDATA[%phoneNumber%]]> oder <![CDATA[<a href="mailto:%supportEmail%">%supportEmail%</a>]]> zur Verfügung. </target>
            </trans-unit>     
            <trans-unit id="5">
                <source>CONFIRM_MESSAGE4</source>
                <target>Vielen Dank für Ihr Interesse an unseren Dienstleistungen.</target>
            </trans-unit>
            <trans-unit id="6">
                <source>CONFIRM_REGARDS</source>
                <target>Best regards</target>
            </trans-unit>
            <trans-unit id="7">
                <source>CONFIRM_SUBJECT</source>
                <target>Your Registration</target>
            </trans-unit>
            <trans-unit id="8">
                <source>EMAIL_CONFIRMATION</source>
                <target>E-mail Confirmation</target>
            </trans-unit>
            <trans-unit id="9">
                <source>EMAIL_CONFIRMATION_MSG1</source>
                <target>Your e-mail hase been verified!</target>
            </trans-unit>
            <trans-unit id="10">
                <source>EMAIL_CONFIRMATION_MSG2</source>
                <target>You E-mail address (%userEmail%) has now been verified and you can begin working in the Fairgate solution.</target>
            </trans-unit>
            <trans-unit id="11">
                <source>EMAIL_CONFIRMATION_MSG3</source>
                <target>Log in</target>
            </trans-unit>
            <trans-unit id="12">
                <source>EMAIL_CONFIRMATION_MSG4</source>
                <target>Start adding contact and reading the help</target>
            </trans-unit>
            <trans-unit id="13">
                <source>EMAIL_CONFIRMATION_BTN</source>
                <target>Go to the login of my new club</target>
            </trans-unit>
            <trans-unit id="14">
                <source>EMAIL_CONFIRMATION_FAILED_MSG1</source> 
                <target>There was an error in the process!</target>
            </trans-unit>
            <trans-unit id="15">
                <source>EMAIL_CONFIRMATION_FAILED_MSG2</source>
                <target>You E-mail address (%userEmail%) could not be verified. Either the link you used was incorrect or misspelled or you or someone else already called the confirmation from that E-mail </target>
            </trans-unit>
            <trans-unit id="16">
                <source>EMAIL_CONFIRMATION_FAILED_MSG3</source>
                <target>Check the link in your email again</target>
            </trans-unit>
            <trans-unit id="17">
                <source>EMAIL_CONFIRMATION_FAILED_MSG4</source>
                <target>Call Fairgate support</target>
            </trans-unit>
            <trans-unit id="18">
                <source>EMAIL_CONFIRMATION_FAILED_BTN</source>
                <target>Go to the support center</target>
            </trans-unit>
            <trans-unit id="19">
                <source>REGISTRATION_REPORT</source>
                <target>Registration Report</target>
            </trans-unit>
            <trans-unit id="20">
                <source>SALES_NOTIFICATION_MESSAGE</source>
                <target>A new club-account was created by the user %userName% (%userEmail%) for the club %clubName% at %dateTime%.</target>
            </trans-unit>
            <trans-unit id="21">
                <source>LINK</source>
                <target>Link: </target>
            </trans-unit> 
            <trans-unit id="22">
                <source>SALES_NOTIFICATION_MESSAGE_LOGIN</source>
                <target>Client %userName% (%userEmail%) from the club %clubName% has first logged into the solution at %dateTime%.</target>
            </trans-unit>             
        </body>
    </file>

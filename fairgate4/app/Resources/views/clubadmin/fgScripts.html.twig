{% if (include == 'jqueryAndUi') %}

    <!-- BEGIN JQUERY & UI JS -->
    {% if (asseticEnv != 'dev') %}
        {% javascripts '@jqueryAndUi' package="assetic" output='js/jqueryAndUi.js' %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% else %}
        <script src="{{ asset('fgassets/assets/global/plugins/jquery-1.11.0.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('fgassets/assets/global/plugins/jquery-migrate-1.2.1.min.js') }}" type="text/javascript"></script>

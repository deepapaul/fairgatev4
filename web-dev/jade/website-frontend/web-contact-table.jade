block content
        .row.fg-web-page-title-bar
            .container
                .fg-web-titlebar-left-col.has-breadcrumb.pull-left

                      .fg-web-page-title-block.fg-breadcrumb-block
                          ul.fg-breadcrumbs
                            li
                              a(href='http://192.168.0.39:12004/auto/home') Title bar improvements
                            li
                              a(href='javascript:void(0);') CLUB

                      .fg-titlebar-block.fg-web-page-title-block
                          h1.fg-web-page-title
                            | How to contact us

                .fg-web-titlebar-right-col.pull-right

                      .fg-titlebar-block.fg-icon-bg
                            i.fg-icon.fg-page-prev

                      .fg-titlebar-block.fg-icon-bg
                            i.fg-icon.fg-page-close

                      .fg-titlebar-block.fg-icon-bg
                            i.fg-icon.fg-page-next
                .clearfix

